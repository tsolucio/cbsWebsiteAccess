<tables>
<table>
<name>vtiger_cbswebsiteaccess</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbswebsiteaccess` (
  `cbswebsiteaccessid` int(11) DEFAULT NULL,
  `websiteaccessno` varchar(100) DEFAULT NULL,
  `access_date` date DEFAULT NULL,
  `access_time` time DEFAULT NULL,
  `relid` int(11) DEFAULT NULL,
  `access_url` varchar(100) DEFAULT NULL,
  `access_web` varchar(100) DEFAULT NULL,
  `sitevisit` varchar(3) DEFAULT NULL,
  `websiteid` varchar(100) DEFAULT NULL,
  `extcmpid` varchar(100) DEFAULT NULL,
  `cmpid` int(11) DEFAULT NULL,
  PRIMARY KEY (`cbswebsiteaccessid`),
  index (`relid`),
  index (`access_date`,`access_time`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
